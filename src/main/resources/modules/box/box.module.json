{
    "name":"Box.Net",
    "namespace":"box",
    "shared-config-snippet-uri":"box-shared-config.xml.snippet",
    "shared-params":[
        {
            "name":"box.api.key",
            "description":"This is the API key generated by Box.net for your App.  For instructions on how to get this please see: http://dddd",
            "type":"password"
        },
        {
            "name":"box.folder",
            "description":"The box folder to work with",
            "type":"text"
        }
    ],
    "templates":[
        {
            "name":"Box.Net write Files",
            "description":"Write files to Box",
            "intent":"publish",
            "input-types":["text/uri-list"],
            "output-types":["text/uri-list"],
            "snippet-uri":"box-write-files.xml.snippet",
            "actions":[
                {
                    "name":"Write Files",
                    "description":"Write files to Box",
                    "params":[
                        {
                            "name":"box.folder",
                            "value":"mulebox"
                        }
                    ]
                }
            ]
        }
    ]
}